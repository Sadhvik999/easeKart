{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "node_modules/.prisma/client/**",
          "prisma/schema.prisma"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/app.js" }
  ]
}